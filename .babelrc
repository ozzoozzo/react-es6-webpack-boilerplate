{
	"presets": [
		"es2015",
		"stage-0",
		"react"
	],
	"plugins": [
		"react-html-attrs"
	],
	"env": {
		"development": {
			"plugins": [
				"transform-runtime",
				["react-transform", {
					"transforms": [{
						"transform": "react-transform-hmr",
						"imports": ["react"],
						"locals": ["module"]
					}]
				}]
			]
		}
	}
}
